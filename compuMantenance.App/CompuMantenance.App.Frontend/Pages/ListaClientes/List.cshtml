@page
@model CompuMantenance.App.Frontend.Pages.ListModelListaClientes
@{
}

<h1 class = "mt-3 bg-secondary text-light text-center rounded">Listado de Clientes</h1>

<table>
    @foreach (var contacto in Model.clientes)
    {
        <tr>
            <td>@contacto.Nombre</td>
            <td>
                <a class = "btn btn-primary table-btn" asp-page = "./Details" asp-route-clienteId = "@contacto.Id">
                <i class = "fas fa-info-circle"></i>
                Detalle Cliente
                </a>

            </td>
        </tr>
    }

</table>
